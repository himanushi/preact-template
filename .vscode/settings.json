{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".*$",
        "cmd": "npx @biomejs/biome check --apply ${file}"
      },
      {
        "match": "\\.css$|\\.html$|\\.md$",
        "cmd": "npx prettier --write ${file}"
      }
    ]
  },
  "cSpell.words": ["clipboardy"]
}
